<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/observablescrollview"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="6dp">
        <include layout="@layout/material_view_pager_placeholder"/>



        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginBottom="30dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingLeft="15dp"
                    android:background="@color/colorSecondary">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:textColor="@color/colorWhite"
                        android:text="Informasi Alamat"/>
                </LinearLayout>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_alamat_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="Alamat ID"


                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_alamat_id"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:text="Sendang Kucing"
                        android:layout_height="wrap_content"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_kecamatan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="Nama"

                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_kecamatan"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:layout_height="wrap_content"
                        android:text="Rowosari"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_kelurahan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="Kelurahan/Desa"

                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_kelurahan"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:layout_height="wrap_content"
                        android:text="Sendang Sikucing"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_kota"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="Kota/Kabupaten"

                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_kota"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:text="Kendal"
                        android:layout_height="wrap_content"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_kodepos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="No HP"

                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_kodepos"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:layout_height="wrap_content"
                        android:text="42115"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
                    android:id="@+id/text_rtrw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hasClearButton="false"
                    app:isResponsiveIconColor="false"
                    app:enabled="false"
                    app:labelText="RT/RW"

                    >

                    <studio.carbonylgroup.textfieldboxes.ExtendedEditText
                        android:id="@+id/extended_rtrw"
                        android:layout_width="wrap_content"
                        android:textColor="@color/colorBlackHeader"
                        android:layout_height="wrap_content"
                        android:text="09/12"

                        />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>


            </LinearLayout>

        </androidx.cardview.widget.CardView>



        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_marginBottom="30dp"></androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_marginBottom="30dp"></androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_marginBottom="30dp"></androidx.cardview.widget.CardView>






    </LinearLayout>


</androidx.core.widget.NestedScrollView>




